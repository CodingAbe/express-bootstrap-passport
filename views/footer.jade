footer.footer
  div.container
    div.bs-docs-social
      ul.bs-docs-social-buttons
        li
          iframe.github-btn(src='http://ghbtns.com/github-btn.html?user=twitter&repo=bootstrap&type=watch&count=true', allowtransparency='true', frameborder='0', scrolling='0', width='100px', height='20px')
        li
          iframe.github-btn(src='http://ghbtns.com/github-btn.html?user=twitter&repo=bootstrap&type=fork&count=true', allowtransparency='true', frameborder='0', scrolling='0', width='102px', height='20px')
        li.follow-btn
          a(href='https://twitter.com/twbootstrap', class='twitter-follow-button', data-link-color='#0069D6', data-show-count='true') Follow @twbootstrap
        li.tweet-btn
          a(href='https://twitter.com/share', class='twitter-share-button', data-url='http://twitter.github.com/bootstrap/', data-count='horizontal', data-via='twbootstrap', data-related='mdo:Creator of Twitter Bootstrap') Tweet
